:{"docs":{},"的":{"docs":{},"子":{"docs":{},"类":{"docs":{},"，":{"docs":{},"根":{"docs":{},"据":{"docs":{},"多":{"docs":{},"态":{"docs":{},"特":{"docs":{},"性":{"docs":{},"调":{"docs":{},"用":{"docs":{},"具":{"docs":{},"体":{"docs":{},"子":{"docs":{},"类":{"docs":{},"的":{"docs":{},"返":{"docs":{},"回":{"docs":{},"结":{"docs":{},"果":{"docs":{},"方":{"docs":{},"法":{"docs":{},"，":{"docs":{},"核":{"docs":{},"心":{"docs":{},"代":{"docs":{},"码":{"docs":{},"在":{"docs":{},"简":{"docs":{},"单":{"docs":{},"工":{"docs":{},"厂":{"docs":{},"类":{"docs":{},"中":{"docs":{},"的":{"docs":{},"分":{"docs":{},"支":{"docs":{},"结":{"docs":{},"构":{"docs":{},"。":{"docs":{},"使":{"docs":{},"用":{"docs":{},"这":{"docs":{},"个":{"docs":{},"模":{"docs":{},"式":{"docs":{},"的":{"docs":{},"重":{"docs":{},"点":{"docs":{},"是":{"docs":{},"在":{"docs":{},"创":{"docs":{},"建":{"docs":{},"的":{"docs":{},"具":{"docs":{},"体":{"docs":{},"子":{"docs":{},"类":{"docs":{},"(":{"docs":{},"产":{"docs":{},"品":{"docs":{},")":{"docs":{},"构":{"docs":{},"造":{"docs":{},"方":{"docs":{},"法":{"docs":{},"不":{"docs":{},"是":{"docs":{},"很":{"docs":{},"复":{"docs":{},"杂":{"docs":{},"，":{"docs":{},"同":{"docs":{},"时":{"docs":{},"具":{"docs":{},"体":{"docs":{},"子":{"docs":{},"类":{"docs":{},"的":{"docs":{},"个":{"docs":{},"数":{"docs":{},"不":{"docs":{},"是":{"docs":{},"很":{"docs":{},"多":{"docs":{},"，":{"docs":{},"不":{"docs":{},"然":{"docs":{},"会":{"docs":{},"造":{"docs":{},"成":{"docs":{},"工":{"docs":{},"厂":{"docs":{},"类":{"docs":{},"的":{"docs":{},"逻":{"docs":{},"辑":{"docs":{},"过":{"docs":{},"于":{"docs":{},"复":{"docs":{},"杂":{"docs":{},"。":{"docs":{},"同":{"docs":{},"时":{"docs":{},"这":{"docs":{},"个":{"docs":{},"模":{"docs":{},"式":{"docs":{},"的":{"docs":{},"缺":{"docs":{},"点":{"docs":{},"也":{"docs":{},"在":{"docs":{},"这":{"docs":{},"里":{"docs":{},"，":{"docs":{},"当":{"docs":{},"需":{"docs":{},"要":{"docs":{},"增":{"docs":{},"加":{"docs":{},"新":{"docs":{},"的":{"docs":{},"操":{"docs":{},"作":{"docs":{},"，":{"docs":{},"必":{"docs":{},"须":{"docs":{},"去":{"docs":{},"修":{"docs":{},"改":{"docs":{},"简":{"do